<table id="defenders" class="table table-striped" cellspacing="0" width="100%">
  <thead>
    <tr>
      <th></th>
      <th>Player</th>
      <th>Team</th>
      <th>Position</th>
      <th>GP</th>
      <th>G</th>
      <th>A</th>
      <th>P</th>
      <th>+/-</th>
      <th>PIM</th>
    </tr>
  </thead>
  <tbody>
    <% @defenders.each do |d| %>
      <tr>
        <td></td>
        <td><%= d.name %></td>
        <td><%= d.abr %></td>
        <td><%= d.position %></td>
        <td><%= d.gp %></td>
        <td><%= d.goals %></td>
        <td><%= d.assists %></td>
        <td><%= d.points %></td>
        <td><%= d.plusMinus %></td>
        <td><%= d.pim %></td>
      </tr>
    <% end %>
  </tbody>
</table>
