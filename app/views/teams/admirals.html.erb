<div class="container-fluid">
  <%= render 'teams/partials/teams' %>
  <div class="row games">
    <div class="col-xs-12">
      <div class="col-xs-4">
        <%= render 'teams/partials/admirals/previous' %>
      </div>
      <div class="col-xs-4">
        <%= image_tag("admiralsSquare.png", alt: "West Auckland Admirals", class: "img-responsive center-block") %>
      </div>
      <div class="col-xs-4">
        <%= render 'teams/partials/admirals/upcoming' %>
      </div>
    </div>
  </div>
  <div class="row stats">
    <div class="col-xs-3">
      <%= render 'teams/partials/admirals/points' %>
    </div>
    <div class="col-xs-3">
      <%= render 'teams/partials/admirals/goals' %>
    </div>
    <div class="col-xs-3">
      <%= render 'teams/partials/admirals/assists' %>
    </div>
    <div class="col-xs-3">
      <%= render 'teams/partials/admirals/gk' %>
    </div>
  </div>
</div>
